{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Client/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/(.+)",
      "dest": "Client/dist/$1"
    }
  ]
}